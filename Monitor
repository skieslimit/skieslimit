Select 
[RobotName]
,SUm(ERRORCOUNT) SumOfErrors 
FROM Robot_Run RR
WHERE RR.StartRequestedTime &gt; DateAdd(Day,-1,getdate())
AND ErrorCount &gt; 0
AND ProjectName &lt;&gt; 'Development'
AND RobotName &lt;&gt; 'UPSInOutTimeConsumerFastlane'
Group By RobotName
order by sum(ERRORCOUNT)desc

