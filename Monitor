          <property name="sql" class="String">"SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

DECLARE @THREEWEEKS DATETIME = DATEADD(WEEK, - 1, GETDATE())
DECLARE @ONEWEEK DATETIME = DATEADD(day, - 4, GETDATE())
DECLARE @ONEDAY DATETIME = DATEADD(DAY, - 1, GETDATE())
DECLARE @NOW DATETIME = GETDATE();

WITH X_CTE (ROBOTNAME)
AS (
	SELECT DISTINCT (ROBOTNAME)
	FROM ROBOT_RUN
	WHERE STARTTIME BETWEEN @ONEDAY
			AND @NOW
	)
SELECT DISTINCT (RR.ROBOTNAME)
FROM ROBOT_RUN RR 
LEFT OUTER JOIN X_CTE ON X_CTE.ROBOTNAME = RR.ROBOTNAME 
WHERE RR.STARTTIME BETWEEN @THREEWEEKS
		AND @ONEWEEK
	AND X_CTE.ROBOTNAME IS NULL"</property>
